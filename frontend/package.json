		"postcss-html": "^1.8.0",
		"stylelint": "^16.18.0",
		"stylelint-config-standard": "^38.0.0",
		"stylelint-order": "^6.0.4",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	}
}
